<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="np.com.sachinmaharzan.expensetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wallet"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NavigationHome"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IndividualHomeActivity" />
        <activity android:name=".ExpenseActivity" />
        <activity android:name=".AddExpenseActivity" />
        <activity android:name=".ExpenseListActivity" />
    </application>

</manifest>