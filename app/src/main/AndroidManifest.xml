<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="np.com.sachinmaharzan.expensetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wallet"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NavigationHome"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IndividualHomeActivity" />
        <activity android:name=".ExpenseActivity" />
        <activity android:name=".AddExpenseActivity" />
        <activity android:name=".ExpenseListActivity" />
        <activity android:name=".IncomeActivity" />
        <activity android:name=".AddIncomeActivity" />
        <activity android:name=".IncomeListActivity" />
        <activity android:name=".BalanceActivity" />
        <activity android:name=".CreditActivity" />
        <activity android:name=".AddCreditActivity" />
        <activity android:name=".CreditListActivity" />
        <activity android:name=".DebtActivity" />
        <activity android:name=".AddDebtActivity" />
        <activity android:name=".DebtListActivity" />
        <activity android:name=".CreateGroupActivity" />
        <activity android:name=".GroupHomeActivity" />
        <activity android:name=".AddMemberActivity" />
        <activity android:name=".MemberListActivity" />
        <activity android:name=".GroupExpenseActivity" />
        <activity android:name=".GroupExpenseListActivity" />
        <activity android:name=".AddGroupExpenseActivity" />
        <activity android:name=".PieChartActivity" />
        <activity android:name=".GroupBudgetActivity" />
        <activity android:name=".GroupPieChartActivity"></activity>
    </application>

</manifest>